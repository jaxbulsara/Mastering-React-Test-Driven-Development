    "@faker-js/faker": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-loader": "^8.2.5",
    "eslint": "^8.20.0",
    "express-graphql": "^0.12.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.3",
    "jest-raw-loader": "^1.0.1",
    "prettier": "^2.7.1",
